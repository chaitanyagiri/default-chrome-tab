{
    "manifest_version": 2,
    "name": "Default Tab Switcher",
    "version": "1.0",
    "permissions": [
        "tabs", "activeTab", "storage"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}